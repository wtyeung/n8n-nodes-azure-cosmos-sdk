{
    "name": "n8n-nodes-azure-cosmos-sdk",
    "version": "0.1.5",
    "description": "n8n node for Azure Cosmos DB using official SDK with hybrid search, vector similarity, and Entra ID support",
    "license": "MIT",
    "homepage": "https://github.com/wtyeung/n8n-nodes-azure-cosmos-sdk",
    "keywords": [
        "n8n-community-node-package"
    ],
    "author": {
        "name": "Tim Yeung",
        "email": "tim.yeung@hku.hk"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wtyeung/n8n-nodes-azure-cosmos-sdk.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "postbuild": "npm run link:dev",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "dev:isolated": "N8N_CUSTOM_EXTENSIONS=$PWD npx n8n start",
        "link:dev": "npm install --prefix ~/.n8n-node-cli/.n8n/nodes \"$npm_package_name@file:$PWD\"",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release"
    },
    "files": [
        "dist",
        "LICENSE"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/AzureCosmosSdkApi.credentials.js",
            "dist/credentials/AzureCosmosSdkEntraIdApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Cosmos/AzureCosmosSdk.node.js"
        ]
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "dependencies": {
        "@azure/cosmos": "^4.6.0"
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/node": "^24.7.0",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
